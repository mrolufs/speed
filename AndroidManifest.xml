<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.profiledesign.speed"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />
    
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.profiledesign.speed.AppContext"
        android:allowBackup="true"
        android:icon="@drawable/pd_icon"
        android:label="@string/app_name">
        <activity
            android:name="com.profiledesign.speed.MainActivity"
            android:windowSoftInputMode="adjustPan|stateHidden">

        </activity>
        
        <activity
            android:name="com.profiledesign.speed.TFamilyFilterActivity"
            android:windowSoftInputMode="adjustPan|stateHidden">

        </activity>
        
        <activity
            android:name="com.profiledesign.speed.ResultsActivity">

        </activity>
        
        <activity
            android:name="com.profiledesign.speed.SplashActivity">
            
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>
